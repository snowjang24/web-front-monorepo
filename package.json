{
  "name": "web-front-monorepo",
  "packageManager": "yarn@4.3.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "dev:next-app": "turbo run dev --filter=next-app"
  },
  "devDependencies": {
    "turbo": "^2.0.5"
  }
}
